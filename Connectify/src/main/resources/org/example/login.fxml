<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.LoginController">
   <children>
      <HBox alignment="TOP_CENTER">
         <children>
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#showLogin" text="Logowanie">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="registerBtn" mnemonicParsing="false" onAction="#showRegister" text="Rejestracja">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></HBox>
      <Pane fx:id="Pane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="700.0" prefWidth="300.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Pane>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
