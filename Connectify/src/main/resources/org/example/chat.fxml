<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="chatScene" prefHeight="720.0" prefWidth="1280.0" stylesheets="@styles/chat.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ChatController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#282b30" height="129.0" layoutX="260.0" layoutY="14.0" stroke="BLACK" strokeType="INSIDE" styleClass="rectangle" width="980.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#282b30" height="57.0" layoutX="40.0" layoutY="14.0" stroke="BLACK" strokeType="INSIDE" styleClass="rectangle" width="200.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#282b30" height="508.0" layoutX="260.0" layoutY="164.0" stroke="BLACK" strokeType="INSIDE" styleClass="rectangle" width="980.0" />
      <ListView fx:id="myListView" layoutX="40.0" layoutY="92.0" prefHeight="580.0" prefWidth="200.0" />
      <Label fx:id="account" alignment="CENTER" contentDisplay="CENTER" layoutX="39.0" layoutY="32.0" prefHeight="22.0" prefWidth="200.0" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font></Label>
      <Label fx:id="myLabel" contentDisplay="CENTER" layoutX="272.0" layoutY="97.0" prefHeight="40.0" prefWidth="572.0" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="1130.0" layoutY="25.0" mnemonicParsing="false" onAction="#searchUser" prefHeight="30.0" prefWidth="78.0" text="Szukaj" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TextField fx:id="search" layoutX="282.0" layoutY="25.0" prefHeight="30.0" prefWidth="820.0" promptText="Wyszukaj znajomego" />
      <Label layoutX="1134.0" layoutY="97.0" prefHeight="40.0" prefWidth="62.0" text="Status:" textFill="WHITE">
         <font>
            <Font name="Arial Italic" size="18.0" />
         </font>
      </Label>
      <Circle fx:id="status" fill="DODGERBLUE" layoutX="1208.0" layoutY="117.0" radius="11.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="addUsername" layoutX="946.0" layoutY="102.0" mnemonicParsing="false" onAction="#addUser" prefHeight="30.0" prefWidth="78.0" text="Dodaj" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="rejectUsername" layoutX="1040.0" layoutY="102.0" mnemonicParsing="false" onAction="#rejectUser" prefHeight="30.0" prefWidth="78.0" text="Odrzuć" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="searchError" alignment="CENTER" contentDisplay="CENTER" layoutX="282.0" layoutY="65.0" prefHeight="27.0" prefWidth="820.0" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="deleteUsername" layoutX="1040.0" layoutY="81.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="32.0" prefWidth="78.0" text="Usuń" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>

   </children>
</AnchorPane>
